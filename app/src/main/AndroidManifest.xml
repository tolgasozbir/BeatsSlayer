<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.beatsslayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/beatslayer"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/beatslayer"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:screenOrientation="landscape">
        <activity android:name=".oyunsayfasi" android:theme="@style/Theme.AppCompat.NoActionBar"
            android:screenOrientation="landscape"/>
        <activity android:name=".MainActivity" android:theme="@style/Theme.AppCompat.NoActionBar"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>